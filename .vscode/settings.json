{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.ignoreRegExpList": ["[０-９Ａ-Ｚａ-ｚぁ-んァ-ヶ亜-熙纊-黑]+"],
  "cSpell.enabledLanguageIds": ["typescript", "typescriptreact"],
  "cSpell.words": [
    "chatframe",
    "cofus",
    "flowtype",
    "Higa",
    "messagearea",
    "noopener",
    "noreferrer",
    "Noto",
    "radiogroup",
    "ress",
    "rmiz",
    "stylelint",
    "Supach",
    "superchat",
    "tabler",
    "Tsubasa"
  ]
}
