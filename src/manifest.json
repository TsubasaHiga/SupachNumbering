{
  "manifest_version": 3,
  "name": "example-name",
  "description": "example-description",
  "author": "HigaTsubasa",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://example.com/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ],
  "permissions": [
    "storage"
  ]
}
